<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pefaur.cordovafb" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fb Plugin</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="icons/android/Icon-mdpi.png" density="mdpi" />
        <icon src="icons/android/Icon-hdpi.png" density="hdpi" />
        <icon src="icons/android/Icon-xhdpi.png" density="xhdpi" />
        <icon src="icons/android/Icon-xxhdpi.png" density="xxhdpi" />
        <icon src="icons/android/Icon-xxxhdpi.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="phonegap-facebook-plugin" spec="~0.12.0">
        <variable name="APP_ID" value="1342764925806270" />
        <variable name="APP_NAME" value="TestUnit" />
    </plugin>
</widget>
